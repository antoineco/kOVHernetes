{
  "apiVersion": "v1",
  "kind": "Pod",
  "metadata": {
    "name": "kube-proxy",
    "namespace": "kube-system"
  },
  "spec": {
    "hostNetwork": true,
    "containers": [
      {
        "name": "kube-proxy",
        "image": "__IMAGE__",
        "command": [
          "kube-proxy",
          "--cluster-cidr=172.17.0.0/16",
          "--healthz-bind-address=127.0.0.1",
          "--proxy-mode=iptables",
          "--kubeconfig=/etc/kubernetes/kubeconfig",
          "--profiling=false"
        ],
        "securityContext": {
          "privileged": true
        },
        "ports": [
          {
            "containerPort": 10256,
            "name": "http"
          }
        ],
        "livenessProbe": {
          "httpGet": {
            "host": "127.0.0.1",
            "path": "/healthz",
            "port": "http"
          },
          "initialDelaySeconds": 15,
          "timeoutSeconds": 15
        },
        "volumeMounts": [
          {
            "mountPath": "/etc/kubernetes/kubeconfig",
            "name": "kubeconfig",
            "readOnly": true
          },
          {
            "mountPath": "/etc/kubernetes/tls",
            "name": "k8s-tls",
            "readOnly": true
          },
          {
            "mountPath": "/etc/ssl/certs",
            "name": "ssl-certs",
            "readOnly": true
          },
          {
            "mountPath": "/usr/share/ca-certificates",
            "name": "ca-certs",
            "readOnly": true
          },
          {
            "mountPath": "/lib/modules",
            "name": "kernel-modules",
            "readOnly": true
          }
        ]
      }
    ],
    "volumes": [
      {
        "hostPath": {
          "path": "/etc/kubernetes/kubeconfig-proxy"
        },
        "name": "kubeconfig"
      },
      {
        "hostPath": {
          "path": "/etc/kubernetes/tls"
        },
        "name": "k8s-tls"
      },
      {
        "hostPath": {
          "path": "/etc/ssl/certs"
        },
        "name": "ssl-certs"
      },
      {
        "hostPath": {
          "path": "/usr/share/ca-certificates"
        },
        "name": "ca-certs"
      },
      {
        "hostPath": {
          "path": "/lib/modules"
        },
        "name": "kernel-modules"
      }
    ]
  }
}
